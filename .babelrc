{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel",
        "@babel/plugin-proposal-class-properties",
        [
          "import",
          {
            "libraryName": "antd",
            "libraryDirectory": "es",
            "style": true
          }
        ]
      ]
    },
    "production": {
      "plugins": [["import", {
        "libraryName":"antd", "libraryDirectory": "es", "style": true }]]
    }
  }
}
